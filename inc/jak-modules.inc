/* JAK Modules */
modules:{
    'jak-common':{
        fullpath:'/js/common.js',
        requires:[
            'anim',
            'app',
            'array',
            'collection',
            'console',
            'cookie',
            'datasource',
            'datatype',
            'datatable',
            'datatable-sort',
            'dd',
            'jak-pod-editor',
            'jak-utl-date',
            'jak-widget',
            'jak-yui3-sugar',
            'node-event-simulate',
            'overlay',
            'recordset',
            'resize-plugin',
            'selector-css3',
            'substitute',
            'tabview',
            'yql'
        ]
    },
    /**
     * css
     */
    'jak-css-calendar':{
        fullpath:'/css/widget/calendar.css', 
        type:'css'
    },
    'jak-css-dialogMask':{
        fullpath:'/css/widget/dialogMask.css',
        type:'css'
    },
    'jak-css-mod-about':{
        fullpath:'/css/mod/about.css',
        type:'css'
    },
    'jak-css-mod-act':{
        fullpath:'/css/mod/act.css',
        type:'css'
    },
    'jak-css-mod-grp':{
        fullpath:'/css/mod/grp.css',
        type:'css'
    },
    'jak-css-pod-info':{
        fullpath:'/css/pod/info.css',
        type:'css'
    },
    'jak-css-pod-grp':{
        fullpath:'/css/pod/grp.css',
        type:'css'
    },
    'jak-css-pod-report':{
        fullpath:'/css/pod/report.css',
        type:'css'
    },
    'jak-css-pod-usr':{
        fullpath:'/css/pod/usr.css',
        type:'css'
    },
    'jak-css-pod-userLogon':{
        fullpath:'/css/pod/userLogon.css',
        type:'css'
    },
    /**
     * data
     */
    'jak-db':{
        fullpath:'/db/base.js',
        requires:['jak-db-grp']
    },
    'jak-db-grp':{
        fullpath:'/db/grp/grp.js',
        requires:['jak-common']
    },
    /**
     * modules
     */
    'jak-mod-about':{
        fullpath:'/mod/about.js',
        requires:['jak-common','jak-css-mod-about']
    },
    'jak-mod-act':{
        fullpath:'/mod/act.js',
        requires:['jak-common','jak-css-mod-act']
    },
    'jak-mod-grp':{
        fullpath:'/mod/grp.js',
        requires:['jak-common','jak-css-mod-grp','jak-db']
    },
    /**
     * plugins
     */
    'jak-plugin-ckEditor':{
        fullpath:'/js/plugin/ckeditor/ckeditor.js'
    },
    'jak-plugin-tabViewRemovable':{
        fullpath:'/mod/tabView/pluginRemovable.js',
        requires:['tabview']
    },
    /**
     * pods (independant overlays)
     */
    'jak-pod-editor':{
        fullpath:'/pod/editor.js',
        requires:['jak-common','jak-plugin-ckEditor']
    },
    'jak-pod-grp':{
        fullpath:'/pod/grp.js',
        requires:['jak-common','jak-css-pod-grp','jak-db-grp']
    },
    'jak-pod-info':{
        fullpath:'/pod/info.js',
        requires:['jak-common','jak-css-pod-info','jak-pod-editor','sortable']
    },
    'jak-pod-report':{
        fullpath:'/pod/report.js',
        requires:['jak-common','jak-css-pod-report']
    },
    'jak-pod-userLogon':{
        fullpath:'/pod/userLogon.php',
        requires:['jak-common','jak-css-pod-userLogon','jak-js-sha1']
    },
    'jak-pod-usr':{
        fullpath:'/pod/usr.php',
        requires:['jak-common','jak-css-pod-usr']
    },
    /**
     * utilities
     */
    'jak-js-sha1':{ //secure hash algorithym
        fullpath:'/js/sha1.js'
    },
    'jak-yui3-sugar': {
         fullpath: '/js/yui3-sugar.js',
         requires:['node','transition']
    },
    /**
     * widgets
     */
    'jak-widget':{
        fullpath:'/widget/base.js',
        requires:[
            'jak-common',
            'jak-widget-calendar',
            'jak-widget-dialogMask',
            'jak-widget-list',
            'jak-widget-message'
        ]
    },
    'jak-widget-calendar':{
         fullpath:'/widget/calendar.js',
         requires:['calendar','jak-common','jak-css-calendar']
    },
    'jak-widget-dialogMask':{
         fullpath:'/widget/dialogMask.js',
         requires:['base','node','jak-css-dialogMask']
    },
    'jak-widget-list':{
        fullpath:'/widget/list.js',
        requires:['jak-common']
    },
    'jak-widget-message':{
        fullpath:'/widget/message.js',
        requires:['jak-common']
    }
}
